{
  "dashboard": {
    "title": "Redis Cache Monitoring",
    "tags": ["clipper", "redis", "cache"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "instance",
          "label": "Instance",
          "type": "query",
          "query": "label_values(redis_up, instance)",
          "datasource": "Prometheus",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Redis Uptime",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "redis_uptime_in_seconds{instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 300, "color": "yellow"},
                {"value": 3600, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Connected Clients",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "redis_connected_clients{instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 80, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(redis_memory_used_bytes{instance=~\"$instance\"} / redis_memory_max_bytes{instance=~\"$instance\"}) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 85, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total{instance=~\"$instance\"}[5m]) / (rate(redis_keyspace_hits_total{instance=~\"$instance\"}[5m]) + rate(redis_keyspace_misses_total{instance=~\"$instance\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Commands per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Commands/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Memory Usage Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "redis_memory_used_bytes{instance=~\"$instance\"}",
            "legendFormat": "{{ instance }} - Used",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes{instance=~\"$instance\"}",
            "legendFormat": "{{ instance }} - Max",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Cache Hit Rate Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total{instance=~\"$instance\"}[5m]) / (rate(redis_keyspace_hits_total{instance=~\"$instance\"}[5m]) + rate(redis_keyspace_misses_total{instance=~\"$instance\"}[5m])) * 100",
            "legendFormat": "{{ instance }} - Hit Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100, "label": "Hit Rate %"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 80, "colorMode": "custom", "op": "lt", "fill": true, "line": true, "fillColor": "rgba(234, 112, 112, 0.12)", "lineColor": "rgb(212, 74, 58)"}
        ]
      },
      {
        "id": 8,
        "title": "Keyspace Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }} - Hits",
            "refId": "A"
          },
          {
            "expr": "rate(redis_keyspace_misses_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }} - Misses",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Connected Clients Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "redis_connected_clients{instance=~\"$instance\"}",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Clients"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 100, "colorMode": "critical", "op": "gt", "fill": false, "line": true}
        ]
      },
      {
        "id": 10,
        "title": "Blocked Clients",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "redis_blocked_clients{instance=~\"$instance\"}",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Blocked Clients"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Evicted Keys",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(redis_evicted_keys_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Evictions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Expired Keys",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "rate(redis_expired_keys_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Expirations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 13,
        "title": "Total Keys in Keyspace",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "redis_db_keys{instance=~\"$instance\"}",
            "legendFormat": "{{ instance }} - DB {{ db }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Keys"},
          {"format": "short"}
        ]
      },
      {
        "id": 14,
        "title": "Network I/O",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "rate(redis_net_input_bytes_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }} - Input",
            "refId": "A"
          },
          {
            "expr": "rate(redis_net_output_bytes_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{ instance }} - Output",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "Bps", "label": "Bytes/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 15,
        "title": "Commands by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 44},
        "targets": [
          {
            "expr": "topk(10, rate(redis_commands_total{instance=~\"$instance\"}[5m]))",
            "legendFormat": "{{ instance }} - {{ cmd }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Commands/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 16,
        "title": "Slowlog Entries",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 52},
        "targets": [
          {
            "expr": "redis_slowlog_length{instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 17,
        "title": "RDB Last Save Time",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 52},
        "targets": [
          {
            "expr": "time() - redis_rdb_last_save_timestamp_seconds{instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 3600, "color": "yellow"},
                {"value": 86400, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 18,
        "title": "Instantaneous Ops/sec",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 52},
        "targets": [
          {
            "expr": "redis_instantaneous_ops_per_sec{instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5000, "color": "yellow"},
                {"value": 10000, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  },
  "overwrite": true
}
