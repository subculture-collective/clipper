# Frontend Environment Variables (Production example)

# Backend API base URL
VITE_API_BASE_URL=https://clpr.tv/api/v1

# Base URL for the frontend application
VITE_BASE_URL=https://clpr.tv

# Twitch
VITE_TWITCH_CLIENT_ID=REPLACE_ME

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# Stripe publishable key and prices
VITE_STRIPE_PUBLISHABLE_KEY=REPLACE_ME
VITE_STRIPE_PRO_MONTHLY_PRICE_ID=REPLACE_ME
VITE_STRIPE_PRO_YEARLY_PRICE_ID=REPLACE_ME

# Sentry (optional)
VITE_SENTRY_ENABLED=false
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=production
VITE_SENTRY_RELEASE=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
